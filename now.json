{
  "version": 2,
  "alias": "lit79-platfrom",
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancelation": false
  },
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "backend/index.js", "use": "@now/node" }
  ],
    "routes": [
        { "src": "/api(.*)", "dest": "backend/index.js" }
    ]
}